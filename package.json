{
  "name": "project-template",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vinxi start",
    "build": "vinxi build",
    "serve": "vite preview",
    "test": "vitest run",
    "dev": "vinxi dev",
    "dev-preview": "pnpm run build && pnpm run start",
    "generate": "pnpm dlx @bruhabruh/theme@latest generate",
    "generate:ui": "pnpm dlx @bruhabruh/ui@latest add",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-router": "^1.114.6",
    "@tanstack/react-router-devtools": "^1.114.6",
    "@tanstack/react-start": "^1.114.6",
    "@tanstack/router-plugin": "^1.114.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fast-equals": "^5.2.2",
    "motion": "^12.4.10",
    "react": "^19.0.0",
    "react-aria": "^3.38.0",
    "react-dom": "^19.0.0",
    "react-scan": "^0.2.14",
    "react-stately": "^3.36.0",
    "tailwind-merge": "^3.0.2",
    "vinxi": "^0.5.3"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.1",
    "@bruhabruh/prettier-config": "^1.1.4",
    "@bruhabruh/theme": "^3.6.2",
    "@bruhabruh/tsconfig": "^1.0.4",
    "@bruhabruh/type-safe": "^1.2.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "3",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  },
  "nano-staged": {
    "*.{js,ts,md,json}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  }
}
